
<div>
    <div>
        Bienvenido {{first_name}} {{last_name}}
    </div>
    <div>
        <form action="{{host}}/api/session/logout", method="GET">
            <button value="submit">Logout</button>
        </form>
    </div>
    <div>
    Lista de Usuarios 
    <div id="contenido">
        <table border="1">
            <theader>
                <tr>
                    <th>Nombre</th>
                    <th>Email</th>
                    <th>Rol</th>
                    <th>Opciones</th>
                </tr>
            </theader>
            <tbody id="lista">
                {{#each usuarios}}
                    <tr>
                        <td>{{this.first_name}} {{this.last_name}}</td>
                        <td>{{this.email}}</td>
                        <td>{{this.role}}</td>
                        <td> 
                            
                                <button id="Eliminar" onclick="eliminar('{{host}}','{{this.email}}')">Eliminar Usuario</button>   
                                <button id="RoleUpdate" onclick="premium('{{host}}','{{this.id}}')">Premium</button>                                                
                            
                        </td>
                    </tr>
                {{/each}}
            </tbody>
        </table>
            
        <h5 id="info"></h5>
    </div>
</div>
</div>
<script src="/js/user.js"></script>