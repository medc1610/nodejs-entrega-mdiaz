<div>
    Lista de productos
    <div>
        <table border="1">
            <theader>
                <tr>
                    <th>Title</th>
                    <th>Description</th>
                    <th>Code</th>
                    <th>Price</th>
                    <th>Status</th>
                    <th>Stock</th>
                    <th>Category</th>
                    <th>Thumbnails</th>
                </tr>
            </theader>
            <tbody>
                {{#each products.docs}}
                    <tr>
                        <td>{{this.title}}</td>
                        <td>{{this.description}}</td>
                        <td>{{this.code}}</td>
                        <td>{{this.price}}</td>
                        <td>{{this.status}}</td>
                        <td>{{this.stock}}</td>
                        <td>{{this.category}}</td>
                        <td>{{this.thumbnails}}</td>
                    </tr>
                {{/each}}
            </tbody>
        </table>
       

    </div>
    
</div>
